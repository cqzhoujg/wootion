<launch>
    <node pkg="track_robot" type="track_robot" name="track_robot" respawn="true" output="log" >
        <param name="can_speed" value="500000"/>
        <param name="print_can" value="0"/>
        <param name="sport_mode" value="PP"/>
        <param name="motor_speed" value="0"/>
        <param name="pv_a" value="0.2"/>
        <param name="target_dis" value="10"/>
        <param name="auto_run" value="1"/>
        <param name="run_times" value="1"/>
        <param name="return_record" value="0"/>
        <param name="data_path" value="/home/ros/data/"/>
        <param name="limit_dis" value="1.750"/>
        <param name="scan_title" value="20.0"/>
        <param name="timer_cycle" value="10"/>
        <param name="print_level" value="debug"/>
    </node>
</launch>
